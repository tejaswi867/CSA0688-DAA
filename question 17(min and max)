#include<stdio.h>
int main()
{
	int i,a[10],max,min,n;
	printf("enter size:");
	scanf("%d",&n);
	printf("enter elements:");
	for(i=0;i<n;i++)
	scanf("%d",&a[i]);
	max=a[0];
	min=a[0];
	for(i=0;i<n;i++)
	{
	if(a[i]>max)
	max=a[i];
	if(a[i]<min)
	min=a[i];
	}
	printf("max element:%d",max);
	printf("\nmin element:%d",min);
}
